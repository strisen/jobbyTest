             <textarea class="form-control" id="chatInput" name="body" placeholder="Say Something..."></textarea><br>
             <button class="btn btn-default" type="submit">Submit</button><br><br>
           </div>
           <div class="well" id="chatUI">
           <div class="well" id="chatWindow" style="overflow: auto; height: 100px;">
           </div>
           </div>
         </form>
       </div>
     </div>
     <div class="col-md-4">
       <h3>Chat Users</h3>
       <ul class="list-group">
         <div id="users"></div>
